{
  "eslint.enable": true,
  "javascript.preferences.importModuleSpecifier": "relative",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#1c1a68",
    "activityBar.background": "#1c1a68",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#b3302d",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#1c1a68",
    "statusBar.background": "#11103f",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#1c1a68",
    "statusBarItem.remoteBackground": "#11103f",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#11103f",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#11103f99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "11103F",
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true
}
